{
  "name": "DeonteDriver",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean": "cd android && ./gradlew clean",
    "reset": "rm -rf node_modules yarn.lock package-lock.json && watchman watch-del-all || true && rm -rf ios/Pods ios/Podfile.lock android/.gradle android/app/build && yarn cache clean --force && yarn install && cd ios && pod deintegrate && pod install && cd .. && npx react-native-clean-project --include watchman,metro,android,ios",
    "reset:android": "rm -rf node_modules yarn.lock package-lock.json && watchman watch-del-all || true && rm -rf android/.gradle android/app/build && yarn cache clean --force && yarn install && npx react-native-clean-project --include watchman,metro,android",
    "reset:ios": "rm -rf node_modules yarn.lock package-lock.json && watchman watch-del-all || true && rm -rf ios/Pods ios/Podfile.lock ~/Library/Developer/Xcode/DerivedData && yarn cache clean --force && yarn install && cd ios && pod deintegrate && pod install && cd .. && npx react-native-clean-project --include watchman,metro,ios"
  },
